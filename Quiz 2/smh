#include<iostream>
using namespace std;
void RandomDessert();  //void means it dosent return anything


int main() {


}
//definition
void RandomDessert() {
	int num = rand() % 100;
	if (num < 50)
		cout << "you got cake" << endl;
	else if (num < 30)
		cout << "you got a Pie!" << endl;
	else if (num < 20)
		cout << "you got a IceCream!" << endl;

}
