#include<iostream>
using namespace std;


float ConeVol(int radius, int height);


int main() {
	//call
	cout << ConeVol(4, 6);

}

//defintion: tells the function what it dose
float ConeVol(int radius, int height) {
	 return V = 3.14 * r2 * h3 / 2;
		

}
