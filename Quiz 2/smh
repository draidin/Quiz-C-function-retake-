#include<iostream>
using namespace std;

//declaration name parameters(), return
bool isAppleOrBannana(string word);

int main() {
	//call
cout << isAppleOrBannana("is this a Apple or bannana");
}

//definition
bool isAppleOrBannana (string word) {
	if (word == "Apple")
		return apple;
	else
		return bannana;

}
